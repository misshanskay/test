<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@taglib uri="http://www.springframework.org/tags/form" prefix="form" %> <!-- springmvc的from标签 -->
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<script type="text/javascript">
	function goback(){
		document.frm.action="${pageContext.request.contextPath}/releaseinfo/editAll.do";
		document.frm.submit();
	}
</script>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>发布信息添加页面</title>
</head>
<body>
<form action="${pageContext.request.contextPath}/releaseinfo/add.do" id="frm" name="frm">
	项目名称：<input type="text" id="title" name="title"><br/>
	项目类型：<input type="text" id="itemType" name="itemType"><br/>
	合作模式：<select id="symbiosis" name="symbiosis">
				<option>请选择</option>
				<option value="1">合资</option>
				<option value="2">独资</option>
				<option value="3">合作</option>
			</select><br/>
	项目所属行业：<input type="text" id="itemBusiness" name="itemBusiness"><br/>
	项目地区：<select id="itemArea" name="itemArea">
				<option>请选择省市</option>
				<option value="北京">北京</option>
				<option value="上海">上海</option>
				<option value="天津">天津</option>
			</select>&nbsp;&nbsp;
			<select id="city" name="city">
				<option value="全部">全部</option>
				<option value="1">...</option>
				<option value="2">...</option>
				<option value="3">...</option>
			</select><br/>
	发布位置：<input type="checkbox" id="releasePosition" name="releasePosition" value="WEB">WEB
	<input type="checkbox" id="releasePosition" name="releasePosition" value="信息化大屏">信息化大屏
	<input type="checkbox" id="releasePosition" name="releasePosition" value="自助终端机">自助终端机
	<input type="checkbox" id="releasePosition" name="releasePosition" value="APP">APP<br/>
	投资总额：<input type="text" id="fundBudget" name="fundBudget"><br/>		
	项目主要内容：<input type="text" id="itemContent" name="itemContent"><br/>		
	联系人：<input type="text" id="linkman" name="linkman"><br/>		
	联系电话：<input type="text" id="phone" name="phone"><br/>		
	联系邮箱：<input type="text" id="email" name="email"><br/>		
	联系地址：<input type="text" id="address" name="address"><br/>		
	传真号码：<input type="text" id="faxnumber" name="faxnumber"><br/>		
	<input type="submit" value="保存">&nbsp;&nbsp;<input type="button" value="返回" onclick="goback()">
</form>
</body>
</html>