<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>入账扣款</title>
<script type="text/javascript" src="${pageContext.request.contextPath}/js/jquery-2.2.3.min.js"></script>
<script type="text/javascript">
		function submits(){
			var username=$("#username").val();
			var deal1=$("#deal1").val();
			var deal2=$("#deal2").val();
			var productTyep=$("#productTyep").val();
			
			if(username=='请输入用户名'){
				$("#username").val("");
			}
			 if(deal1!="请输入余额"&&deal2!="请输入余额"){
					if(deal1.isNaN||deal2.isNaN){
						return false;
					}		
			}
			 
			 if(deal1=="请输入余额"&&deal2=="请输入余额"){
				 $("#deal1").val("");
				 $("#deal2").val("");
			 }
			 
			 if(productTyep=='请输入项目名称'){
				$("#productTyep").val("");
			}
			document.getElementById("frm").action="${pageContext.request.contextPath}/booked/query.do";
			document.getElementById("frm").submit();
		}
</script>
</head>
<body style="align: center;">
<form action="" id="frm">
	<div style="margin-top:50px; width:800px; border:1px solid #F00; margin-left: 200px">
			  用户名：<input type="text" id="username" name="username" value="请输入用户名" onfocus="if (value =='请输入用户名'){value =''}"onblur="if (value ==''){value='请输入用户名'}"><br/>
			 余     额：<input type="text" id="deal1" name="dealOne" value="请输入余额" onfocus="if (value =='请输入余额'){value =''}"onblur="if (value ==''){value='请输入余额'}">
			 -<input type="text" id="deal2" name="dealTwo"  value="请输入余额" onfocus="if(value=='请输入余额') {value=''}" onblur="if (value ==''){value='请输入余额'}"><br/>
			 项目名称：<input type="text" id="productTyep" name="productType" value="请输入项目名称" onfocus="if(value=='请输入项目名称') {value=''}" onblur="if (value ==''){value='请输入项目名称'}"><br/>
      	  	<input type="button" value="查找" onclick="submits()">
     </div>
</form>

</body>
</html>