package cn.sirbox.dao;

import java.util.List;

import org.apache.ibatis.annotations.Param;
import org.springframework.stereotype.Repository;

import cn.sirbox.model.Part;

@Repository
public interface PartMapper {
    
   
    
    List<Part> getAllPart();
    
    Part getPartByName(String pname);
    Part getPartById(Integer pid);

	void addPart(@Param("part")Part part);

	void addPartRole(@Param("pname")String pname, @Param("rname")String rname);

	List<String> getRole(String pname);

	void deleteRole(String pname);

	void updataPart(@Param("part")Part part);

    void insertUserUPart(@Param("uid")Integer uid, @Param("pid")Integer pid);
}