<%@ page language="java" contentType="text/html; charset=utf-8"
    pageEncoding="utf-8"%>
    <%@taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<title>Insert title here</title>
<link rel="stylesheet" type="text/css" href="${pageContext.request.contextPath}/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="${pageContext.request.contextPath}/css/basestyle.css">
     <script src="${pageContext.request.contextPath}/js/jquery-2.2.3.min.js"></script>
    <script src="${pageContext.request.contextPath}/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="${pageContext.request.contextPath}/js/moment.js"></script>
    <script type="text/javascript" src="${pageContext.request.contextPath}/My97DatePicker/WdatePicker.js"></script>
</head>
<body>
<form role="form" action="${pageContext.request.contextPath }/enterprise/upenterprise1.do" enctype="multipart/form-data" method="post">
<input type="hidden" name="eid" value="${enterprise.eid }">
    <div class="form-group">
        <label for="name">用户名</label>
        <input type="text" class="form-control-6" name="ename" id="name" value="${enterprise.ename }">
        <label id="namemessage"></label>
    </div>
    <div class="form-group">
        <label for="password">密码</label>
        <input type="password" class="form-control-6" name="epassword" id="password" value="${enterprise.epassword }">
    </div>
    <div class="form-group">
         <label for="password1">确认密码</label>
         <input type="password" class="form-control-6" id="password1" value="${enterprise.epassword }">
        <label id="passwordmessage"></label>
     </div>

     <div class="form-group">
    <label for="nome">企业名称</label>
    <input type="text" class="form-control-6" name="enome" id="nome" value="${enterprise.enome }">
    <button>获取企业信息</button>
    </div>
     <div class="form-group">
    <img src="${pageContext.request.contextPath}/images/enterprise/${enterprise.ebusiness}" width="200" height="80"/>
    <label for="business">营业执照号</label>
    <input type="file" class="form-control-6" name="business" id="business">
    </div>
    <div class="form-group">
    <img src="${pageContext.request.contextPath}/images/enterprise/tex/${enterprise.etex}" width="200" height="80"/>
    <label for="tex">税务登记号</label>
    <input type="file" class="form-control-6" name="tex" id="tex">
    </div>
    <div class="form-group">
         <label for="amount">注册金额</label>
         <input type="text" class="form-control-6" id="amount" name="eamount" value="${enterprise.eamount }">
        <label for="amount">万元</label>
     </div>
     <div class="form-group">
         <label for="legal">企业法人</label>
         <input type="text" class="form-control-6" id="legal" name="elegal" value="${enterprise.elegal }">
        
     </div>
      
    <div class="form-group">
        <label>注册地址</label>
        <select class="form-control-6" name="eaddress">
            <option value="请选择省市">请选择省市</option>
                            <c:forEach var="list" items="${map.province}">
                            <option <c:if test="${enterprise.eaddress==list}">selected</c:if>>${list }</option>
                            </c:forEach>
            
        </select>
    </div>
    
   
     <div class="form-group">
        <label for="time">注册时间</label>
       <%-- <input class="laydate-icon" name="ubirthday" onclick="laydate()"><br /><br />--%>
        <input type="text" id="time" name="etime" onfocus="WdatePicker({dateFmt: 'yyyy-MM-dd HH:mm:ss'});"
        value="${enterprise.etime }"/>

    </div>
     <div class="form-group">
         <label for="scope">经营范围</label>
         <input type="text" class="form-control-6" id="scope" name="escope" value="${enterprise.escope }">
        
     </div>
     <div class="form-group">
         <label for="contacts">联系人</label>
         <input type="text" class="form-control-6" id="contacts" name="econtacts" value="${enterprise.econtacts }">
        
     </div>
     <div class="form-group">
         <label for="phone">联系电话</label>
         <input type="text" class="form-control-6" id="phone" name="ephone" value="${enterprise.ephone }">
        
     </div>
    <div class="form-group">
        <label for="email">邮箱地址</label>
        <input type="text" class="form-control-6" name="eemail" id="email" value="${enterprise.eemail }">
    	<label id="emailmessage"></label>
    </div>
    
    <div class="form-group">
        <label>招商意向</label>
        <select class="form-control-6" name="eintention">
             <option selected="" value="请选择">请选择</option>
                            <option <c:if test="${enterprise.eintention=='找项目'}">selected</c:if>>找项目</option>
                            <option <c:if test="${enterprise.eintention=='找资金'}">selected</c:if>>找资金</option>
                            <option <c:if test="${enterprise.eintention=='找技术'}">selected</c:if>>找技术</option>
                            <option <c:if test="${enterprise.eintention=='找厂址'}">selected</c:if>>找厂址</option>
                            <option <c:if test="${enterprise.eintention=='技术输出'}">selected</c:if>>技术输出</option>
                            <option <c:if test="${enterprise.eintention=='投资输出'}">selected</c:if>>投资输出</option>
                            <option <c:if test="${enterprise.eintention=='其他'}">selected</c:if>>其他</option>
            
        </select>
    </div>
    
    <div class="form-group">
        <label>行业类型</label>
        <select class="form-control-6" name="etype">
            <option value="请选择">请选择</option>
            				<c:forEach var="list" items="${map1.type}">
                            <option <c:if test="${enterprise.etype==list}">selected</c:if>>${list }</option>
                            </c:forEach>
            
        </select>
    </div>
    

    <button type="submit" class="btn btn-default">修改</button>
</form>

</body>
<script>

    
    $("#password1").change(function(){
        if($("#password").val()==$("#password1").val()){
            $("#passwordmessage").html("");
        }else{

            $("#passwordmessage").html("两次密码不一致！");
        }

    })
    $("#password").change(function(){
        if($("#password").val()==$("#password1").val()){
            $("#passwordmessage").html("");
        }else{

            $("#passwordmessage").html("两次密码不一致！");
        }

    })
    
	   $("#email").blur(function(){
	   var email = $(this).val();
	   var reg = /\w+[@]{1}\w+[.]\w+/;
	   if(reg.test(email)){
	    $("#emailmessage").html("");
	   }else{
	    $("#emailmessage").html("请输入正确的email地址");
	   }
	  }); 

</script>
</html>