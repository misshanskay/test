package cn.sirbox.dao;

import java.util.List;

import org.apache.ibatis.annotations.Param;
import org.springframework.stereotype.Repository;

import cn.sirbox.model.Dept;
@Repository
public interface DeptMapper {
   
    int updateByPrimaryKey(Dept record);

	List<Dept> getAllDept();

	void updateDeptById(@Param("did")Integer did,@Param("pid")Integer pid);

	void deleteDeptById(Integer did);

	void addDept(@Param("dept")Dept dept);

	void updateDept(@Param("dept")Dept dept);

	Dept getDeptByName(@Param("dname")String dname);
}