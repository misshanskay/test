<%@ page language="java" contentType="text/html; charset=utf-8"
    pageEncoding="utf-8"%>
    <%@taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<title>Insert title here</title>
<link rel="stylesheet" type="text/css" href="${pageContext.request.contextPath}/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="${pageContext.request.contextPath}/css/basestyle.css">
    <script src="${pageContext.request.contextPath}/js/jquery-2.2.3.min.js"></script>
    <script type="text/javascript" src="${pageContext.request.contextPath}/js/moment.js"></script>
</head>
<body>
<a href="${pageContext.request.contextPath}/enterprise/addenterprise.do">添加企业</a>
<table class="table">
<tr>
<td>选择</td>
<td>企业名称</td>
<td>联系人</td>
<td>注册地</td>
<td>注册资金</td>
<td>未来5年投融资意向</td>
<td>状态</td>
<td>操作</td>
</tr>
<c:forEach var="list" items="${list}">
        <tr><td>
        <input type="checkbox" name="check" value="${list.eid }"/></td>
            <td>${list.enome}</td>
            <td>${list.econtacts}</td>
            <td>${list.eaddress}</td>
            <td>${list.eamount}</td>
            <td>${list.eintention}</td>
            <td>
            <c:if test="${list.estatus==1}"><span style="color:red">待审核</span></c:if>
            <c:if test="${list.estatus==2}"><span style="color:green">已审核</span></c:if>
            <c:if test="${list.estatus==3}"><span style="color:yellow">未通过</span></c:if>
            </td>
            
            <td>
            	
            <c:if test="${list.estatus==1}"><a href="${pageContext.request.contextPath}/user/updata.do?uid=${list.eid}">审核</a></c:if>
            <c:if test="${list.estatus==2}"><a href="${pageContext.request.contextPath}/user/updata.do?uid=${list.eid}">查看</a></c:if>
            <c:if test="${list.estatus==3}"><a href="${pageContext.request.contextPath}/user/updata.do?uid=${list.eid}">查看</a></c:if>
            
                <a href="${pageContext.request.contextPath}/enterprise/upenterprise.do?eid=${list.eid}">修改</a>
                <button onclick="dele(${list.eid},${thispage})">删除</button>
            </td>
        </tr>
    </c:forEach>
    <tr>
        <td></td>
        <td>
            <button id="up" onclick="up(${thispage})">上一页</button>
            <button id="down" onclick="down(${thispage})">下一页</button>
            共${thispage}/${page1}页
        </td>
        <td></td>
    </tr>
    </table>
</body>
</html>