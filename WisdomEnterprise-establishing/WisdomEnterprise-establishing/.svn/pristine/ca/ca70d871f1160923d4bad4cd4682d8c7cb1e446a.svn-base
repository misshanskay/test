<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
    <%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<title>Insert title here</title>
<link rel="stylesheet" type="text/css" href="${pageContext.request.contextPath}/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="${pageContext.request.contextPath}/css/basestyle.css">
    <script src="${pageContext.request.contextPath}/js/jquery-2.2.3.min.js"></script>
    <script type="text/javascript" src="${pageContext.request.contextPath}/js/moment.js"></script>
</head>
<body>
<table class="table">
<tr>
    <td>用户名</td><td>${user.uname }</td>
    </tr>
    <tr>
    <td>性别</td><td>${user.userExtend.usex }</td>
    </tr>
    <tr>
    <td>邮箱地址</td><td>${user.userExtend.uemail }</td>
    </tr>
    <tr>
    <td>手机号码</td><td>${user.userExtend.uphone }</td>
    </tr>
    <tr>
    <td>头像</td><td><img src="${pageContext.request.contextPath}/${user.userExtend.imgsrc}" width="200" height="80"/></td>
    </tr>
    <tr>
    <td>真实姓名</td><td>${user.userExtend.urealname }</td>
    </tr>
    <tr>
    <td>出生年月</td><td><fmt:formatDate value="${user.userExtend.ubirthday }" type="date" pattern="yyyy-MM-dd"/></td>
    </tr>
    <tr>
    <td>岗位</td><td>${user.userExtend.upost }</td>
    </tr>
    <tr>
    <td>就职单位</td><td>${user.userExtend.uunit }</td>
    </tr>
    <tr>
    <td>身份证</td><td>${user.userExtend.ucard }</td>
    </tr>
    <tr>
    <td>政治面貌</td><td>${user.userExtend.upolitical }</td>
    </tr>
    <tr>
    <td>地址</td><td>${user.userExtend.uaddress }</td>
    </tr>
    <tr>
    <td>毕业院校</td><td>${user.userExtend.ugraduation }</td>
    </tr>
    <tr>
    <td>专业</td><td>${user.userExtend.umajor }</td>
    </tr>
    <tr>
    <td>个人描述</td><td>${user.userExtend.uintroduction }</td>
    </tr>
    </table>
</body>
<script type="text/javascript">
$(document).ready(function(){
	
});
</script>
</html>